{
    "manifest_version": 3,
    "name": "YouTube Channel Live Checker",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "alarms"],
    "host_permissions": [
        "*://*.youtube.com/*"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
}
